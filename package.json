    "immer": "^10.0.4",
    "next": "14.1.2",
    "next-intl": "^3.9.4",
    "react": "^18",
    "react-dom": "^18",
    "react-type-animation": "^3.2.0",
    "styled-components": "^6.1.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.18",
    "@playwright/test": "^1.42.1",
    "@storybook/addon-essentials": "^7.6.17",
