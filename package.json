  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix",
      "npm run lint:fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
